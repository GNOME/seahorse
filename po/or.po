#: ../daemon/seahorse-daemon.c:69
msgid "Do not run seahorse-daemon as a daemon"
msgstr "seahorse-daemon କୁ ଗୋଟିଏ ଡେମନ ଭାବରେ ଚଳାନ୍ତୁ ନାହିଁ"

#: ../daemon/seahorse-daemon.c:87
msgid "couldn't fork process"
msgstr "ପ୍ରକ୍ରିୟା ମାନଙ୍କୁ fork କରିପାରିଲା ନାହିଁ"

#: ../daemon/seahorse-daemon.c:93
msgid "couldn't create new process group"
msgstr "ନୂତନ ପ୍ରକ୍ରିୟା ସମୂହ ସୃଷ୍ଟି କରିପାରିଲା ନାହିଁ"

#: ../daemon/seahorse-daemon.c:207
msgid "Encryption Daemon (Seahorse)"
msgstr "ଗୋପନୀୟ ଡେମନ (Seahorse)"

#: ../daemon/seahorse-daemon.desktop.in.in.h:1
msgid "Seahorse Daemon"
msgstr "Seahorse ଡେମନ"

#: ../daemon/seahorse-service.c:151 ../daemon/seahorse-service.c:175
#, c-format
