<ui>
    <menubar>
        <menu name="Key" action="key-menu">
            <menuitem action="key-import-keyring"/>
            <menuitem action="key-export-file"/>
            <separator/>
            <menuitem action="remote-find"/>
            <separator/>
            <menuitem action="key-properties"/>
            <separator/>
            <menuitem action="app-close"/>
        </menu>
        <menu name="Edit" action="edit-menu">
            <menuitem action="key-export-clipboard"/>
        </menu>
        <menu name="Help" action="help-menu">
            <menuitem action="help-show"/>
<!--        <menuitem action="app-about"/> -->
        </menu> 

    </menubar>
  
    <toolbar name="MainToolbar">
        <placeholder name="ToolItems">
            <toolitem action="key-import-keyring"/>
            <toolitem action="key-export-file"/>
            <toolitem action="key-properties"/>
        </placeholder>
    </toolbar>

    <popup name="KeyPopup">
        <menuitem action="key-export-clipboard"/>
        <separator/>
        <menuitem action="key-import-keyring"/>
        <menuitem action="key-export-file"/>
        <separator/>        
        <menuitem action="key-properties"/>
    </popup>
    
</ui>
